{
  "name": "koa2-web-engine",
  "version": "1.0.0",
  "description": "koa2的web引擎demo",
  "main": "app.ts",
  "scripts": {
    "watch"      : "ts-node ./app.ts",
    "start"      : "nodemon --watch ./app.js",
    "build"      : "tsc",
    "debugger"   : "nodemon  --watch ./src -e  ts,tsx  --exec node --inspect -r ts-node/register  ./app.ts",
